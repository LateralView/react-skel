{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 8,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
  },
  "rules": {
    "no-extra-semi": 0,
    "no-mixed-spaces-and-tabs": 0,
    "jest/valid-expect": 0,
    "linebreak-style": ["error", "unix"]
  }
}
